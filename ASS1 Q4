#include <stdio.h>

int main() {
    int a, b, C;
    printf("Enter two numbers: ");
    scanf("%d %d", &a, &b);
//1ST WAY
    C = a;
    a = b;
    b = C;
//2ND WAY
    a = a + b;
    b = a - b;
    a = a - b;
//3RD WAY
    a = a ^ b;
    b = a ^ b;
    a = a ^ b;
//4TH WAY
    a=a*b;
    b=a/b;
    a=a/b;



    printf("After swapping: a = %d, b = %d\n", a, b);
    return 0;
}
